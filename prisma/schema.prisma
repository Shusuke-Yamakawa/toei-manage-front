// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model card {
  card_id String @id
  password String
  user_nm String
  available_flg Boolean @default(true)
  note String
  courts getCourt[]
}

model draw {
  id Int @default(autoincrement()) @id
  card_id String
  year Int
  month Int
  day Int
  from_time Int
  to_time Int
  court String
  confirm_flg Boolean
}

model oddsCourt {
  id Int @default(autoincrement()) @id
  year Int
  month Int
  day Int
  from_time Int
  to_time Int
  court String
  odds Int
}

model getCourt {
  id Int @default(autoincrement()) @id
  card_id String
  year Int
  month Int
  day Int
  from_time Int
  to_time Int
  court String
  card card @relation(fields: [card_id], references: [card_id])
}